    <style>
        /* deshboard card */
        .card-content .revenue:hover {

            box-shadow: 4px 4px 4px darkgoldenrod;
        }

        .card-single:hover {
            transform: translate(0, -8px);
        }

        /* for profile */

        .link a:hover {
            color: rgb(2, 2, 71);
        }

        .drop-item .dropdown-toggle {
            transform: translateY(0);
            opacity: 1;
            visibility: visible;

        }

        .drop-content {
            box-shadow: 2px 2px 2px darkorange;
            transform: translateY(50px);
            opacity: 0;
            visibility: hidden;
            transition: .3s;
        }


        .drop-item .drop-content::before {

            position: absolute;
            left: 86%;
            top: -7px;
            background-color: rgb(2, 2, 71);
            width: 14px;
            height: 14px;
            transform: translate(50%) rotate(45deg);
        }

        /* for Notification */



        .link a:hover {
            color: rgb(2, 2, 71);
        }

        .social-icon .notifi_dropdown {
            transform: translateY(0);
            opacity: 1;
            visibility: visible;

        }

        .notification-content {
            transform: translateY(50px);
            opacity: 0;
            visibility: hidden;
            transition: .3s;
        }


        .social-icon .notification::before {

            position: absolute;
            left: 86%;
            top: -7px;
            background-color: rgb(2, 2, 71);
            width: 14px;
            height: 14px;
            transform: translate(50%) rotate(45deg);
        }

        /* for sidebar */

        .sidebar {
            transition: width 500ms;
        }


        .b-btn:hover {
            background: rgb(2, 2, 71);
            color: darkorange;
        }

        .b-btn:hover {
            background: rgb(2, 2, 71);
            color: darkorange;
        }

        /* form 1 for */
        .f1-btn {
            background: rgb(2, 2, 71);
            color: darkorange;
        }

        .f1-btn:hover {
            background: darkorange;
            color: rgb(2, 2, 71);
        }

        .submit-btn input {
            background: darkorange;
            color: rgb(2, 2, 71);
        }

        .submit-btn input:hover {
            background: rgb(2, 2, 71);
            color: darkorange;
        }

        .box .file input:hover {
            border: 2px solid rgb(2, 2, 71);
        }

        .box input:focus {
            border: 2px solid rgb(2, 2, 71);
        }

        .box .company:focus {
            border: 2px solid rgb(2, 2, 71);

        }

        .error {
            font-weight: 700;
            display: block;
            color: #f00;
            font-size: 12px;
            text-align: left;
            padding-left: 1rem;
            position: relative;
        }
    </style>
